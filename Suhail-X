FROM quay.io/CkodtChanu/Methu-Md-v1
RUN git clone https://quay.io/CKODTCHANU/METHU-MD-v0.01 /root/smdd
RUN rm -rf /root/smdd/.git
WORKDIR /root/smdd
RUN npm install || yarn install
EXPOSE 8000
CMD ["npm","start" ] 
































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































